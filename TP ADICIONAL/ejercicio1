Algoritmo ejercicio1
	
	Definir potencia, elFin, elInicio, cuenta, suma, contador, elevado Como Real;
	
	elInicio = 1;
	cotador = 0;
	
	
	Escribir "Cuantas veces queres potenciar?";
	Leer elFin;
	
	para i = elInicio hasta elFin con paso 1;
		contador = contador + 1;
		elevado= 2^contador;
		
		Escribir elevado;
	FinPara
	
	
FinAlgoritmo
